[Unit]
Description=Headphones - Automatic music downloader for SABnzbd

[Service]
ExecStart=/usr/bin/python /usr/share/headphones/Headphones.py --daemon --config /etc/headphones/headphones.ini --datadir /var/headphones/.headphones --nolaunch --quiet
GuessMainPID=no
Type=forking
User=headphones
Group=headphones

[Install]
WantedBy=multi-user.target
